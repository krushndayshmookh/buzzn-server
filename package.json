{
  "name": "buzzn-server",
  "version": "1.0.0",
  "description": "Backend template for Root Kings Express Applications",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint --ext .js . --fix ",
    "prettify": "prettier --write '**/*{.js,.json}'",
    "hooks:pre-commit": "node ./hooks/pre-commit.js",
    "hooks:pre-push": "node ./hooks/pre-push.js",
    "prepare": "husky install"
  },
  "husky": {
    "pre-commit": "npm run hooks:pre-commit",
    "pre-push": "npm run hooks:pre-push"
  },
  "author": "Krushn Dayshmookh <krushndayshmookh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1171.0",
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.4.4",
    "mongoose-lean-virtuals": "^0.9.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pug": "^3.0.2",
    "shelljs": "^0.8.5",
    "socket.io": "^4.5.1",
    "stripe": "^9.12.0",
    "unique-string": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0"
  }
}
